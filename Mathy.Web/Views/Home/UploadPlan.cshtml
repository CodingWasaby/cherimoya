@{
    if (ViewData["Message"] != null)
    { 
        <div class="bordered">
            @ViewData["Message"]
        </div>
        <br />
    }
}

@using (Html.BeginForm("UploadPlan", "Home", FormMethod.Post, new
    {
        enctype = "multipart/form-data"
    }))
{
    
    <input type="hidden" name="planID" id="planID" value="@ViewData["PlanID"]" />

    <input type="file" id="file" name="file" value="选择文件" class="btn btn-primary" />

    <br />

    <input type="submit" value="上传" class="btn btn-primary" />
}
